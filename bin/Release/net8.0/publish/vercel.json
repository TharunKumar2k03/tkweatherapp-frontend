{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "wwwroot/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/_framework/(.+)",
      "dest": "/_framework/$1",
      "headers": {
        "content-type": "application/wasm"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}